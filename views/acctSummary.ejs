<%
acctData.data.acct.sort(function(a, b) {
    if (a.inodesused < b.inodesused) return -1;
    if (a.inodesused > b.inodesused) return 1;
    return 0;
  });
%>
<div class="export-btn-section">
    <div class="nav-head">Account Summary</div>
<div class="export-btn" title="Export to pdf">
    <button onclick="HtmlTOExcel('xlsx','acct-summary')"><img src="/images/file-export-solid.svg"/></button>
</div>
</div>

<div class="main">
<table id="acct-summary" class="table table-dark">
    <thead>
    <tr>
        <th>Inode User</th>
        <th>Domain</th>
        <th>User</th>
        <th>Email</th>
        <th>Start Date</th>
        <th>Disk Used</th>
    </tr>
    </thead>
    <tbody>
    <% acctData.data.acct.forEach(s=>{ %>
    <tr>
        <td><%= s.inodesused %></td>
        <td><%= s.domain %></td>
        <td><%= s.user %></td>
        <td><%= s.email %><l</td>
        <td><%= s.startdate %></td>
        <td><%= s.diskused %></td>
    </tr>
    <%})%>
</tbody>
    
</table>

</div>